{
  "name": "mat-service",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3100",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev -p 3100",
    "build": "next build",
    "start": "next start",
    "preflight": "npm run lint && npm run build && npm run test:unit && npm run prettier:test",
    "serverless:offline": "npm run build && serverless offline -p 3200 -s staging",
    "test:unit": "jest --config ./jest.unit.config.js",
    "test:unit:ci": "jest --config ./jest.unit.config.js -w 2",
    "test:unit:watch": "jest --config ./jest.unit.config.js --watch",
    "test:unit:coverage": "jest --config ./jest.unit.config.js --coverage",
    "test:integration": "jest --config ./jest.integration.config.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prettier:test": "npx prettier -c .",
    "prettier:fix": "npx prettier --write .",
    "db:start": "docker-compose up -d db && ./db/wait_for_postgres.sh && npm run db:migrate:up -e development up",
    "db:migrate": "db-migrate --migrations-dir 'db/migrations/'",
    "db:migrate:up": "db-migrate --migrations-dir 'db/migrations/' up",
    "db:migrate:test": "db-migrate --migrations-dir 'db/migrations/' -e test",
    "db:migrate:dry-run": "db-migrate --dry-run --migrations-dir 'db/migrations/'",
    "db:seed": "cat db/migrations/sqls/seeds.sql | docker-compose exec -T db psql -U MatServiceDbAdmin matServiceDb",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:open:dev": "cypress open --env JWT_SECRET=secret",
    "cypress:run:dev": "cypress run  --env JWT_SECRET=secret"
  },
  "dependencies": {
    "@types/classnames": "^2.2.10",
    "@types/cookie": "^0.4.0",
    "@types/faker": "^4.1.12",
    "@types/history": "^4.7.6",
    "@types/js-cookie": "^2.2.6",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/moment": "^2.13.0",
    "@types/react-icons": "^3.0.0",
    "@types/react-router-dom": "^5.1.5",
    "cookie": "^0.4.1",
    "dotenv-load": "^2.0.0",
    "faker": "^4.1.0",
    "govuk-frontend": "^3.7.0",
    "history": "^5.0.0",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "^2.2.1",
    "lbh-frontend": "^3.0.0",
    "lbh-frontend-react": "^0.5.5",
    "moment": "^2.27.0",
    "next": "9.4.4",
    "next-absolute-url": "^1.2.2",
    "next-env": "^1.1.1",
    "node-lambda-authorizer": "LBHackney-IT/node-lambda-authorizer.git#master",
    "pg-promise": "^10.5.8",
    "pkcs7-padding": "^0.1.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-icons": "^3.11.0",
    "react-router-dom": "^5.2.0",
    "restana": "^4.6.1",
    "rijndael-js": "^2.0.0",
    "sass": "^1.26.10",
    "serve-static": "^1.14.1",
    "serverless-http": "^2.5.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "@types/pkcs7-padding": "^0.1.0",
    "@types/react": "^16.9.41",
    "@types/rijndael-js": "^1.0.0",
    "@types/serve-static": "^1.13.5",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "cypress": "^4.11.0",
    "db-migrate": "^0.11.11",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.1.0",
    "jsdom": "16.3.0",
    "jsdom-global": "3.0.2",
    "node-mocks-http": "^1.8.1",
    "prettier": "^2.1.1",
    "pretty-quick": "^3.0.2",
    "serverless": "^1.74.1",
    "serverless-dotenv-plugin": "^2.4.2",
    "serverless-offline": "^6.4.0",
    "serverless-offline-scheduler": "^0.4.2",
    "serverless-offline-ssm": "^5.0.1",
    "serverless-plugin-typescript": "^1.1.9",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.7"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
